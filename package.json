{
  "name": "image-to-gcode",
  "version": "0.1.0",
  "private": true,
  "main": "electron-dist/main.js",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "preflight": "yarn build && yarn vitest && yarn lint && yarn tsc",
    "build:electron": "tsc -p electron/tsconfig.json",
    "watch:electron": "tsc -p electron/tsconfig.json -w",
    "electron:dev": "concurrently -k \"yarn dev\" \"yarn watch:electron\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:prod": "cross-env NODE_ENV=production yarn build && yarn build:electron && electron .",
    "electron:build": "cross-env NODE_ENV=production yarn build && yarn build:electron && electron-builder"
  },
  "dependencies": {
