    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "three": "^0.179.1",
    "use-image": "^1.1.4",
    "write-file-atomic": "^6.0.0",
    "zeromq": "^6.5.0"
  },
  "build": {
    "appId": "com.path-generator",
    "productName": "Path Generator",
    "files": [
      "electron-dist/**/*",
      ".next/**/*",
      "next.config.*",
      "package.json"
    ],
    "extraMetadata": {
      "main": "electron-dist/main.js"
    },
    "mac": {
      "category": "public.app-category.productivity"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
