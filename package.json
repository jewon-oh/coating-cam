    "@types/write-file-atomic": "^4.0.3",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "electron": "^37.2.6",
    "electron-builder": "^26.0.12",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "redux-devtools-extension": "^2.13.9",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5",
    "vitest": "^3.2.4",
    "wait-on": "^8.0.4"
